<?xml version="1.0"?>
<extension key="com.joineryhq.reltoken" type="module">
  <file>reltoken</file>
  <name>Related Tokens</name>
  <description>Provides tokens for any related contact, for all available contact tokens. E.g., to get the spouse's first name, or parent's marriage date.</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://github.com/twomice/com.joineryhq.reltoken</url>
    <url desc="Documentation">http://github.com/twomice/com.joineryhq.reltoken</url>
    <url desc="Support">http://github.com/twomice/com.joineryhq.reltoken/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2022-07-11</releaseDate>
  <version>0.4.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Reltoken</namespace>
    <format>24.09.1</format>
  </civix>
  <requires>
    <ext>org.civicrm.flexmailer</ext>
  </requires>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CiviMix\Schema\Reltoken\AutomaticUpgrader</upgrader>
  <mixins>
    <mixin>entity-types-php@2.0.0</mixin>
  </mixins>
</extension>
